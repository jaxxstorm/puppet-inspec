{
  "name": "jaxxstorm-inspec",
  "version": "0.1.1",
  "author": "jaxxstorm",
  "summary": "A puppet module to install Chef inspec",
  "license": "MIT",
  "source": "https://github.com/jaxxstorm/puppet-inspec",
  "project_page": "https://github.com/jaxxstorm/puppet-inspec",
  "issues_url": "https://github.com/jaxxstorm/puppet-inspec/issues",
